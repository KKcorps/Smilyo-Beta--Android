<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smilyo.main"
    android:versionCode="5"
    android:versionName="1.1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="11" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/smilyo_logo_white"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.smilyo.main.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.smilyo.main.Body"
            android:label="@string/app_name" />
        <activity
            android:name="com.smilyo.main.CollegePortal"
            android:label="@string/app_name" />
        <activity
            android:name="com.smilyo.main.RecruitmentsJuly"
            android:label="@string/app_name" />
    </application>

</manifest>